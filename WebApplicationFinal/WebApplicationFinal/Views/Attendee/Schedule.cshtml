@model EventPlanner.Models.VoteViewModel
@{
    ViewBag.Title = "Schedule";
}

<h2>Schedule</h2>
<table class="table" style="color:#ffffff;">
    <tr>
        <th>Entertainment Name</th>
        <th>Points</th>
    </tr>
    @foreach (var entertainment in Model.entertainments)
    {
        <tr>
            <td>
                <div>
                    <p> @entertainment.Name</p>
                </div>
            </td>
            <td>
                <div>
                    <p>@entertainment.VenueId</p>
                </div>
            </td>
        </tr>
    }
</table>

<p id="reply">
    <button>Get Forecast</button>

</p>

<script>
    $(document).ready(function () {
        $("button").click(function () {
            $.get("http://api.openweathermap.org/data/2.5/weather?q=Milwaukee&APPID=cea53ed56c3ee5fc14e6d1120b4e9e9d", function (response) {
                //response
                $("#reply").text(response.main.temp));
            });
        });
    });
</script>